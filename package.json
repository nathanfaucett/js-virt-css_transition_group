    "has": "git://github.com/nathanfaucett/has",
    "request_animation_frame": "git://github.com/nathanfaucett/request_animation_frame.git",
    "supports": "git://github.com/nathanfaucett/supports",
    "virt": "git://github.com/nathanfaucett/virt",
